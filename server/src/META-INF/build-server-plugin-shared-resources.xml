<?xml version="1.0" encoding="UTF-8"?>
<!--suppress AutowiredDependenciesInspection -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-autowire="constructor">


  <!-- ===  BUILD FEATURE ===-->


  <bean class="jetbrains.buildServer.sharedResources.server.SharedResourcesBuildFeature"/>

  <bean class="jetbrains.buildServer.sharedResources.server.SharedResourcesPluginController"/>

  <bean class="jetbrains.buildServer.sharedResources.server.BuildFeatureParametersProvider"/>

  <bean class="jetbrains.buildServer.sharedResources.server.SharedResourcesWaitPrecondition"/>



  <!-- ===  INTERNALS  === -->

  <bean class="jetbrains.buildServer.sharedResources.server.FeatureParamsImpl"/>

  <bean class="jetbrains.buildServer.sharedResources.server.feature.SharedResourcesFeaturesImpl"/>

  <bean class="jetbrains.buildServer.sharedResources.pages.SharedResourcesPage"/>

  <bean class="jetbrains.buildServer.sharedResources.settings.PluginSettingsManager"/>

  <bean class="jetbrains.buildServer.sharedResources.pages.SharedResourcesActions"/>

</beans>